#include "Ethernet.h"

void Ethernet_init(){

	//set PCENET in PCONP
	LPC_SC->PCONP |=PCONP_PCENET_MASK;

	// clock config

	//PINSEL config
	#define GPIO_PORT 1
	#define GPIO_FUNC 1
	GPIO_configDigitalGeneric(GPIO_PORT, GPIO_FUNC, uint32_t dir, uint32_t mask, uint32_t initValue){
}

